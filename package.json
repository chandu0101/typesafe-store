{
  "private": true,
  "name": "typesafe-store",
  "version": "1.0.0",
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "scripts": {
    "build-clean": "rm -rf ./packages/*/build ./packages/*/build-es5 ./packages/*/tsconfig.tsbuildinfo",
    "build": "tsc --build --incremental tsconfig.build.json",
    "publish": "yarn build-clean && yarn build && lerna publish from-package",
    "reset": "rm -rf node_modules packages/*/node_modules",
    "test": "jest"
  },
  "dependencies": {
    "@types/node": "^12.12.7",
    "lerna": "^3.18.3",
    "ts-essentials": "^3.0.4",
    "ts-node": "^8.4.1",
    "ttypescript": "^1.5.7",
    "typescript": "3.7.2"
  }
}
